{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": ["eslint:recommended", "plugin:prettier/recommended"],
    "plugins": ["prettier"],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "prettier/prettier": "warn",
        // "eqeqeq": "warn",
        "no-async-promise-executor": "off",
        // "no-unused-vars": "off"
        "no-unused-vars": ["error", { "argsIgnorePattern": "next" }]
    }
  }